name: Node.js CI

on:
  push:
    branches: [ "Maicol" ]
    
jobs:
  build:
    runs-on: ubuntu-latest

    steps:
    - uses: actions/checkout@v4
    - name: Use apphola.js
      uses: actions/setup-node@v4
      with:
        node-version: '18.x'
    - run: npm install express
    - run: node node.js
